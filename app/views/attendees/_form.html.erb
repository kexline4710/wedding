<%= form_for @attendee do |f| %>
  <p>
    <%= f.label :first_name %><br/>
    <%= f.text_field :first_name %>
  </p>

  <p>
    <%= f.label :last_name %><br/>
    <%= f.text_field :last_name %>
  </p>

  <p>
    <%= f.label :email %><br/>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :full_address %><br/>
    <%= f.text_field :full_address %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
